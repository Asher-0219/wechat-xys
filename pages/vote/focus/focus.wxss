.header{
    height: 100rpx;
    width: 100%;
    color: #ff2e78;
    line-height: 100rpx;
    font-size: 28rpx;
    padding-right: 24rpx;
    text-align: right;
    box-sizing: border-box;
}
.kong{
    width: 100%;
    height: 20rpx;
    background: #f5f5f7;
}
checkbox{
    width: 35rpx;
    height: 35rpx;
    border-radius: 50%;
}
.at-list{
    padding: 25rpx 24rpx 0 20rpx;
}
.at-title{
    font-size: 24rpx;
    color: #969696;
}
.at-icon{
    width: 100rpx;
    height: 100rpx;
    display: inline-block;
    border-radius: 10rpx;
}
.at-sex-icon{
    width: 64rpx;
    height: 32rpx;
}